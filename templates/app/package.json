{
  "id": "<%= app %>",
  "name": "<%= appname %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "author": {
    "name": "<%= author.email %>",
    "email": "<%= author.name %>"
  },
  "contributors": [
    {}
  ],
  "scripts": {
    "start": "grunt fun",
    "test": "grunt test",
    "jspm": "jspm i",
