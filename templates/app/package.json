    "postinstall": "npm run jspm && npm run typings install && grunt init",
    "typings": "typings"
    <% } %>
    <% if ( isBabel ) { %>
    "postinstall": "npm run jspm && grunt init"
    <% } %>
  },
  "repository": "<%= git %>",
  "keywords": [
    "angular"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.6",
